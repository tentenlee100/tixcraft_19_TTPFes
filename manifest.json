{
  "name": "Team TP 3rd 握手會相關拓元頁面簡化",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Team TP 3rd 握手會相關拓元頁面簡化",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://tixcraft.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tixcraft.com/*"
      ],
      "js": [
        "scripts/jquery-3.4.1.min.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}


